33.Fix the code to return the Prime numbers................................................
const newArray=[1,2,3,5,10];
let array=[];
function data(a){

    if(a<2){
        return false;
    }
    for(let n of a){
        for(let i =2;i<n;i++){
            if(n%i===0){
                return false;
            }else{
                array.push(n);
                break;

            }
        }

    
    
}
}
data(newArray);
console.log(array)
